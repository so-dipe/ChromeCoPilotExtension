{
    "manifest_version": 3,
    "name": "Chrome CoPilot",
    "version": "0.0.3",
    "description": "A Chrome extension that helps you navigate the web",
    "action": {},
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "permissions": [
        "sidePanel"
    ],
    "side_panel": {
        "default_path": "panel.html"
    }
}